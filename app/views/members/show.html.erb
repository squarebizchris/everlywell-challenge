<div class="row">
  <div class="col-12">
    <p>Name: <%= @member.name %></p>
    <p>Url: <%= @member.url %></p>
    <p>Short Url: <%= @member.short_url %></p>
  </div>
</div>
<div class="row">
  <div class="col-6">
    Friends
  </div>
  <div class="col-6">
    <h3>Headings</h3>
    <ul>
      <% @headings.each do |heading| %>
        <li><%= heading.header_text %></li>
      <% end %>
    </ul>
  </div>
</div>
